            <xsd:element minOccurs="0" name="StartDateTime" type="cct:DateTimeType">
                <xsd:annotation>
                    <xsd:documentation>Start Datetime of the Time Period</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:choice>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Duration" type="cct:DurationType">
                        <xsd:annotation>
                            <xsd:documentation>Duration of the Time Period</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="EndDateTime" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>End Date Time of the Time Period</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:choice>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="NoteType">
        <xsd:annotation>
            <xsd:documentation>Notes related to the entity</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Content" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Conent of the Notes</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="AgentID" type="cct:IDType">
                        <xsd:annotation>
                            <xsd:documentation>Agent or user who updated or created the notes</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="CreatedDateTime" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the note was created.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="LastModifiedDateTime" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>Last Modified time of the notes</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PriceType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Amount" type="cct:AmountType">
                <xsd:annotation>
                    <xsd:documentation>The Amount</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DiscountAmount" type="cct:AmountType">
                <xsd:annotation>
                    <xsd:documentation>The discount Amount</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DiscountPercent" type="cct:PercentType">
                <xsd:annotation>
                    <xsd:documentation>The discount percent</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="OneOffPriceType">
        <xsd:complexContent>
            <xsd:extension base="tns:PriceTypeWithID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Activation" type="tns:PriceType">
                        <xsd:annotation>
                            <xsd:documentation>The total one-off activation price. The currency must be set.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Installation" type="tns:PriceType">
                        <xsd:annotation>
                            <xsd:documentation>The total one-off installation price. The currency must be set.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="NetAmount" type="cct:AmountType">
                        <xsd:annotation>
                            <xsd:documentation>The net amount for the one-off price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="TaxAmount" type="cct:AmountType">
                        <xsd:annotation>
                            <xsd:documentation>The tax amount for the one-off price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="RecurringPriceType">
        <xsd:complexContent>
            <xsd:extension base="tns:PriceTypeWithID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Activation" type="tns:PriceType">
                        <xsd:annotation>
                            <xsd:documentation>The total one-off activation price. The currency must be set.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Frequency" type="tns:PriceFrequencyType">
                        <xsd:annotation>
                            <xsd:documentation>The frequency at which the recurring price is applied</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="NetAmount" type="cct:AmountType">
                        <xsd:annotation>
                            <xsd:documentation>The net amount for the recurring price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="TaxAmount" type="cct:AmountType">
                        <xsd:annotation>
                            <xsd:documentation>The tax amount for the recurring price</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="PriceFrequencyType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="IntervalQuantity" type="cct:QuantityType">
                <xsd:annotation>
                    <xsd:documentation>The interval between the time elements. The unit code must be set, e.g. "Day", "Month", etc.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IntervalsQuantity" type="cct:QuantityType">
                <xsd:annotation>
                    <xsd:documentation>The number of time the interval between the time elements repeats.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IntervalUnitCode" type="cct:CodeType">
                <xsd:annotation>
                    <xsd:documentation>The interval unit code.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="IndividualOrOrganisationPartyRoleReferenceType">
        <xsd:annotation>
            <xsd:documentation>A type defining a Party Role reference component to Party that can be defined as either an Individual or an Organisation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:choice minOccurs="0">
                        <xsd:element minOccurs="1" name="Individual" type="tns:IndividualRoleType">
                            <xsd:annotation>
                                <xsd:documentation>A component describing the Individual Party (Person) that is undertaking the role.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="1" name="Organisation" type="tns:OrganisationPartyType">
                            <xsd:annotation>
                                <xsd:documentation>A component describing the Organisation Party that is udnertaking the role.s</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:choice>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="IndividualAndOrganisationPartyRoleReferenceType">
        <xsd:annotation>
            <xsd:documentation>A type defining a Party Role reference component to Party that can be defined as either an Individual and an Organisation.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Individual" type="tns:IndividualRoleType">
                        <xsd:annotation>
                            <xsd:documentation>A component describing the Individual Party (Person) that is undertaking the role.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Organisation" type="tns:OrganisationPartyType">
                        <xsd:annotation>
                            <xsd:documentation>A component describing the Organisation Party that is udnertaking the role.s</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="StatusHistoryType">
        <xsd:annotation>
            <xsd:documentation>A history of the lifecycle state transistions of a VBO. Each transition records the date and time and should record the target lifecycle status.</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="StateTransition">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element minOccurs="0" name="ID" type="cct:IDType">
                            <xsd:annotation>
                                <xsd:documentation>An identifier or name for the state transition. Examples include: "Activate", "Cancel", "Complete", etc.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="FromStatusCode" type="cct:CodeType">
                            <xsd:annotation>
                                <xsd:documentation>The source lifecycle state of the transition, i.e. the state from which the transition is moving, for example: from "Active" to "Cancelled".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="ToStatusCode" type="cct:CodeType">
                            <xsd:annotation>
                                <xsd:documentation>The target lifecycle state of the transition, i.e. the state to which the transition is moving, for example: from "Active" to "Cancelled".</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="EffectiveDateTime" type="cct:DateTimeType">
                            <xsd:annotation>
                                <xsd:documentation>The date and time of the transistion, i.e. the timestamp of when the target state was entered.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="ReasonCode" type="cct:CodeType">
                            <xsd:annotation>
                                <xsd:documentation>The code describing why the transition occured.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="ReasonText" type="cct:TextType">
                            <xsd:annotation>
                                <xsd:documentation>The reason code describing why the transition occured</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="SubReasonCode" type="cct:CodeType">
                            <xsd:annotation>
                                <xsd:documentation>The Sub reason code describing why the transition occured.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="SubReasonText" type="cct:TextType">
                            <xsd:annotation>
                                <xsd:documentation>The sub-reason code describing why the transition occured</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="Description" type="cct:TextType">
                            <xsd:annotation>
                                <xsd:documentation>A description of the state transition.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="ChangedByUserID" type="cct:IDType">
                            <xsd:annotation>
                                <xsd:documentation>A reference to the User [Account] who initiated the lifecycle state transition.</xsd:documentation>
                            </xsd:annotation>
                        </xsd:element>
                    </xsd:sequence>
                    <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
                        <xsd:annotation>
                            <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:attribute>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="GeographicLocationType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="AltitudeMeasure" type="cct:MeasureType">
                <xsd:annotation>
                    <xsd:documentation>The measure of the altitude that reflects the vertical elevation of an object above a surface for this geographical coordinate (Reference ISO 6709</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LatitudeMeasure" type="cct:MeasureType">
                <xsd:annotation>
                    <xsd:documentation>The measure of the latitude as an angular distance north or south from the Equator meridian to the meridian of a specific place for this geographical coordinate. (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LongitudeMeasure" type="cct:MeasureType">
                <xsd:annotation>
                    <xsd:documentation>The measure of the longitude as an angular distance east or west from the Greenwich meridian to the meridian of a specific place (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LatitudeDirectionCode" type="cct:CodeType">
                <xsd:annotation>
                    <xsd:documentation>The indication of whether the latitude compass direction from the Equator meridian to the meridian of a specific place is North (+) or South (-). (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LongitudeDirectionCode" type="cct:CodeType">
                <xsd:annotation>
                    <xsd:documentation>The indication of whether the longitude as a compass direction from the Greenwich meridian to the meridian of a specific place is East (-) or West (+) for this geographical coordinate. (Reference ISO 6709)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SystemID" type="cct:IDType">
                <xsd:annotation>
                    <xsd:documentation>A unique identifier of the system used for measuring a geographical coordinate other than Global Positioning System</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="CredentialsGroupType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="PasswordText" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>The password associated to the user.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PasswordHintText" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>A hint to help the user to remember his/her password.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="UsernameText" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>The user name being used by the user.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PINs">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="PIN" type="tns:PINType"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PasswordExpireDate" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>The date when the password is going to expire.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PasswordWarningDate" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>The date when a warning is going to be sent to the user, letting him/her know that the password is about to expire.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PasswordExpired" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>An indicator stating if the password is expired or not.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="PasswordWarned" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>An indicator stating if the password expired warning has been sent to the user.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
