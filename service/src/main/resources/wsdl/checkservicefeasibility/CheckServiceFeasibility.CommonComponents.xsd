                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AccountNumber" type="cct:NumericType">
                <xsd:annotation>
                    <xsd:documentation>The bank account number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SortCode" type="cct:NumericType">
                <xsd:annotation>
                    <xsd:documentation>The bank account sort code</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IBAN" type="cct:NumericType">
                <xsd:annotation>
                    <xsd:documentation>The bank account IBAN</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BIC" type="cct:TextType">
