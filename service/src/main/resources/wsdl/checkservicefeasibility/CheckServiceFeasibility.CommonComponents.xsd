    <xsd:complexType name="MembersRoleType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="Member" type="tns:MemberRoleType"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="MemberRoleType">
        <xsd:complexContent>
            <xsd:extension base="tns:PartyReferenceType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Role" type="cct:IDType"/>
                    <xsd:element minOccurs="0" name="Influence" type="cct:IDType"/>
                    <xsd:element minOccurs="0" name="Opinion" type="cct:TextType"/>
                    <xsd:element minOccurs="0" name="ReportsTold" type="cct:IDType"/>
                    <xsd:element minOccurs="0" name="Urgency" type="cct:TextType"/>
                    <xsd:element minOccurs="0" name="BusinessGoals" type="cct:TextType"/>
                    <xsd:element minOccurs="0" name="DecisionCriteria" type="cct:TextType"/>
                    <xsd:element minOccurs="0" name="PersonalGoals" type="cct:TextType"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ContactPeopleType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ContactPerson">
                <xsd:complexType>
                    <xsd:complexContent>
                        <xsd:extension base="tns:ContactPersonType">
                            <xsd:sequence>
                                <xsd:element minOccurs="0" name="ContactPoints">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ContactPoint" type="tns:ContactPointType"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:extension>
                    </xsd:complexContent>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ContactPointsType">
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" name="ContactPoint" type="tns:ContactPointType">
                <xsd:annotation>
                    <xsd:documentation>Contact Point. The choice being Postal, Telephone, Fax and SMS</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ContactPersonType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="JobFunctions">
                        <xsd:annotation>
                            <xsd:documentation>The set of job functions that the contact person is employed for.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="JobFunction" type="tns:JobFunctionType">
                                    <xsd:annotation>
                                        <xsd:documentation>A specific job function of the contact person</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="JobTitle" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Job Title e.g Manager, CEO, Consultant etc</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Seniority" type="cct:IDType">
                        <xsd:annotation>
                            <xsd:documentation>The seniority of the contact person.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ContactPreferences">
                        <xsd:annotation>
                            <xsd:documentation>The set of contact preferences for the contact person. May include time of day, contact point, form of address, etc.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element maxOccurs="unbounded" name="ContactPreference" type="tns:ContactPersonContactPreferenceType">
                                    <xsd:annotation>
                                        <xsd:documentation>A specific contact preference for a contact person. May include time of day, contact point, form of address, etc.</xsd:documentation>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="IndividualName" type="tns:IndividualNameType">
                        <xsd:annotation>
                            <xsd:documentation>The details of an individuals name.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="BirthDate" type="cct:DateType">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual was born.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="BloodType" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Blood Type of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="DeathDate" type="cct:DateType">
                        <xsd:annotation>
                            <xsd:documentation>Date on which the Individual died.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="DeathIndicator" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>Flag indicating whether the Individual has died</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Ethnicity" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Ethnicity of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Gender" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Gender of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Language" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Langauge of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="MaritalStatus" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Martial status of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Nationality" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>Nationality of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="DesignatedIndicator" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>Designated indicator of the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="TPSRegistration" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>TPS (Telephone preference service) registeration indicator for the Individual</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="FamilyGeneration" type="cct:CodeType">
                        <xsd:annotation>
                            <xsd:documentation xml:lang="EN">An abbreviation or word that pertains to the generation in a family, such Sr, Jr.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="JobFunctionType">
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Name" type="cct:TextType">
                <xsd:annotation>
                    <xsd:documentation>Name of the job function</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Type" type="cct:CodeType">
                <xsd:annotation>
                    <xsd:documentation>Type of the job function</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="actionCode" type="tns:ActionCodeType" use="optional">
            <xsd:annotation>
                <xsd:documentation>The @actionCode attribute is used within a modification operation to indicate the action (add, update, delete) intended on a component.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    <xsd:complexType name="ContactPersonContactPreferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:PreferenceType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== ProductOffering Reference-->
    <xsd:complexType name="ProductOfferingReferenceType">
        <xsd:annotation>
            <xsd:documentation>The presentation of one or more Product Specifications to the marketplace for sale, rental, or lease for a Product Offering Price.
				A Product Offering may target one or more Market Segments, be included in one or more Product Catalogue, presented in support of one or more Product Strategies,
				and made available in one or more Places. Product Offering may represent a simple offering of a single Product Specification or could represent a bundling
				of one or more other Product
				Offering.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== SolutionOffering Reference-->
    <xsd:complexType name="SolutionOfferingReferenceType">
        <xsd:annotation>
            <xsd:documentation>Especially for Enterprise Customers, the product offering that forms the basis of the Quote will be customised for a specific customer.
				This Customised Offering is not the same as a Product although the final 'accepted' customised offering will be the specification for the product build.
				Customised Offerings may go through multiple versions before being accepted.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== Condition Reference-->
    <xsd:complexType name="ConditionReferenceType">
        <xsd:annotation>
            <xsd:documentation>Condition</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <!--===== ProductCatalogue Reference-->
    <xsd:complexType name="ProductCatalogueReferenceType">
        <xsd:annotation>
            <xsd:documentation>A list of ProductOfferings for sale, with prices and illustrations, for example in book form or on the web.
				ProductCatalogs can be used by Customers during a self-care ordering process and may be used across one or more DistributionChannels.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ApprovalReferenceType">
        <xsd:annotation>
            <xsd:documentation>A group of Authorizations required from the Business Participants involved in the entity being approved.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ApprovalType">
        <xsd:annotation>
            <xsd:documentation/>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="tns:ApprovalReferenceType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="CreationDateTime" type="cct:DateTimeType">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was created.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="RequestedDate" type="cct:DateType">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was requested.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="CompletedDate" type="cct:DateType">
                        <xsd:annotation>
                            <xsd:documentation>The date and time the approval was completed.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Outcome" type="cct:TextType">
                        <xsd:annotation>
                            <xsd:documentation>A free text description of the outcome of the approval.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ChannelPreferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Consent" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>An indication of whether the channel preference was given as consent by the consenter.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Channel" type="tns:ChannelReferenceType">
                        <xsd:annotation>
                            <xsd:documentation>The specific channel the preference applies to.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Explicit" type="cct:IndicatorType">
                        <xsd:annotation>
                            <xsd:documentation>An indication of whether explicit permission has been given for the preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ExplicitThreshold" type="cct:NumericType">
                        <xsd:annotation>
                            <xsd:documentation>The threshold value for the explicit permission/preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ValidityPeriod" type="tns:ValidityPeriodType">
                        <xsd:annotation>
                            <xsd:documentation>The period over which the preference is valid.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    <xsd:complexType name="ChannelReferenceType">
        <xsd:complexContent>
            <xsd:extension base="tns:BaseComponentType"/>
        </xsd:complexContent>
