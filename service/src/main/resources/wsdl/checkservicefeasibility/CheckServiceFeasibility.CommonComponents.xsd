                        <xsd:annotation>
                            <xsd:documentation>An indication of whether explicit permission has been given for the preference.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ConfidenceRating" type="cct:NumericType">
                        <xsd:annotation>
